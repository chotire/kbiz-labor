<html xmlns="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:w2="http://www.inswave.com/websquare"
	xmlns:xf="http://www.w3.org/2002/xforms">
	<head>
		<w2:type>DEFAULT</w2:type>
		<w2:buildDate />
		<xf:model>
			<xf:instance>
				<data xmlns="" />
			</xf:instance>
			<w2:dataCollection baseNode="map">
				<w2:dataList baseNode="list" repeatNode="map" id="dsTcmStndJobtypCdTreeDtlList"
					saveRemovedData="true">
					<w2:columnInfo>
						<w2:column id="stndItemiCd" name="표준내역코드" dataType="text"></w2:column>
						<w2:column id="stndItemiNm" name="표준내역명" dataType="text"></w2:column>
						<w2:column id="upperStndItemiCd" name="상위표준내역코드" dataType="text"></w2:column>
						<w2:column id="stndItemiLv" name="표준내역레벨" dataType="text"></w2:column>
						<w2:column id="itemiYn" name="내역여부" dataType="text"></w2:column>
						<w2:column id="stndItemiCdNavi" name="표준내역코드NAVI" dataType="text"></w2:column>
						<w2:column id="stndItemiNmNavi" name="표준내역명NAVI" dataType="text"></w2:column>
						<w2:column id="sortOdrNavi" name="정렬NAVI" dataType="text"></w2:column>
						<w2:column id="itemiYnNext" name="하위레벨 내역여부" dataType="text"></w2:column>
						<w2:column id="upperStndItemiCdNext" name="하위레벨의 상위_표준내역코드" dataType="text"></w2:column>
						<w2:column id="upperLvStndItemiCd" name="상위레벨 표준내역코드" dataType="text"></w2:column>
						<w2:column id="upperLvStndItemNm" name="상위레벨 표준내역명" dataType="text"></w2:column>
					</w2:columnInfo>
				</w2:dataList>
				<w2:dataList baseNode="list" repeatNode="map" id="dsTcmStndJobtypAcctMappList" saveRemovedData="true">
					<w2:columnInfo>
						<w2:column id="chk" name="체크박스" dataType="text"></w2:column>
						<w2:column id="no" name="No" dataType="text"></w2:column>
						<w2:column id="acctCd" name="계정코드" dataType="text"></w2:column>
						<w2:column id="acctNm" name="계정명" dataType="text"></w2:column>
						<w2:column id="stndItemiCd" name="표준내역코드" dataType="text"></w2:column>
					</w2:columnInfo>
				</w2:dataList>
				<w2:dataMap baseNode="map" id="dsTcmStndJobtypCdDtlMap">
					<w2:keyInfo>
						<w2:key id="stndItemiCd" name="표준내역코드" dataType="text"></w2:key>
						<w2:key id="stndItemiNm" name="표준_내역_명" dataType="text"></w2:key>
						<w2:key id="upperStndItemiCd" name="상위_표준내역코드" dataType="text"></w2:key>
						<w2:key id="upperStndItemiNm" name="상위_표준내역명" dataType="text"></w2:key>
						<w2:key id="stndItemiLv" name="표준_내역_레벨" dataType="text"></w2:key>
						<w2:key id="sortOdr" name="정렬_순서" dataType="text"></w2:key>
						<w2:key id="useYn" name="사용_여부" dataType="text"></w2:key>
						<w2:key id="stndItemiUnit" name="표준_내역_단위" dataType="text"></w2:key>
						<w2:key id="itexpGb" name="비목구분" dataType="text"></w2:key>
						<w2:key id="ctrlTypeCd" name="통제유형코드" dataType="text"></w2:key>
						<w2:key id="bgtNewAddYn" name="예산_신규_추가_여부" dataType="text"></w2:key>
						<w2:key id="bgtItemiJnlzYn" name="예산_내역_분개_여부" dataType="text"></w2:key>
						<w2:key id="bgtItexpChgYn" name="예산_비목_변경_여부" dataType="text"></w2:key>
						<w2:key id="bgtAmtVrtnYn" name="예산_금액_증감_여부" dataType="text"></w2:key>
						<w2:key id="bgtMinusWrtYn" name="예산_마이너스_작성_여부" dataType="text"></w2:key>
						<w2:key id="pcostInjtYn" name="원가_투입_여부" dataType="text"></w2:key>
						<w2:key id="itemiYn" name="내역_여부" dataType="text"></w2:key>
						<w2:key id="autoCrtYn" name="자동_생성_여부" dataType="text"></w2:key>
						<w2:key id="acctCtrlYn" name="계정_통제_여부" dataType="text"></w2:key>
						<w2:key id="bgtCtrlYn" name="예산_통제_여부" dataType="text"></w2:key>
						<w2:key id="fstRegprEmpno" name="최초_등록자_사번" dataType="text"></w2:key>
						<w2:key id="fstRegDtm" name="최초_등록_일시" dataType="text"></w2:key>
						<w2:key id="fnlModprEmpno" name="최종_수정자_사번" dataType="text"></w2:key>
						<w2:key id="fnlModiDtm" name="최종_수정_일시" dataType="text"></w2:key>
					</w2:keyInfo>
				</w2:dataMap>
				<w2:dataMap baseNode="map" id="dsSearchParamMap">
					<w2:keyInfo>
						<w2:key id="stndItemiCd" name="표준내역코드" dataType="text"></w2:key>
						<w2:key id="tabGubun" name="탭구분" dataType="text"></w2:key>
					</w2:keyInfo>
				</w2:dataMap>
				<w2:dataList baseNode="list" repeatNode="map" id="dsYnList" saveRemovedData="true">
					<w2:columnInfo>
						<w2:column id="code" name="코드" dataType="text"></w2:column>
						<w2:column id="name" name="코드명" dataType="text"></w2:column>
					</w2:columnInfo>
					<w2:data use="true">
						<w2:row>
							<code><![CDATA[Y]]></code>
							<name><![CDATA[예]]></name>
						</w2:row>
						<w2:row>
							<code><![CDATA[N]]></code>
							<name><![CDATA[아니오]]></name>
						</w2:row>
					</w2:data>
				</w2:dataList>
				<w2:dataList baseNode="list" repeatNode="map" id="dsCdCM004100" saveRemovedData="true">
					<w2:columnInfo>
						<w2:column id="charValN1" name="공통상세코드" dataType="text"></w2:column>
						<w2:column id="cdNm" name="이름" dataType="text"></w2:column>
					</w2:columnInfo>
				</w2:dataList>
				<w2:dataList baseNode="list" repeatNode="map" id="dsCdCM004101" saveRemovedData="true">
					<w2:columnInfo>
						<w2:column id="cd" name="공통상세코드" dataType="text"></w2:column>
						<w2:column id="cdNm" name="이름" dataType="text"></w2:column>
					</w2:columnInfo>
				</w2:dataList>
			</w2:dataCollection>
			<w2:workflowCollection></w2:workflowCollection>
		</xf:model>

		<script type="javascript" lazy="false"><![CDATA[
/***************************************************************************
 * Program ID : CM00150010T.xml
 * Program Name : 현장관리비기준정보 - 표준내역TAB
 * Author : jeong se jun (tplsj)
 * Date : 2019.02.18
 *
 * History
 * ========================================
 * 변경일자     수정자   내 용
 * ========================================
 *
 **************************************************************************/

	/************************************************************************************
		DataCollection description

		dsTcmStndJobtypCdTreeDtlList: 좌측 TREE
		dsTcmStndJobtypCdDtlMap: 표준내역 상세정보 FORM
		dsSearchParamMap: 검색조건 parameter

		:공통코드
		dsYnList: 예[Y], 아니오[N]
		dsCdCM004100: 비목 - 자재비[M], 노무비[L], 외주비[S], 경비[O]
		dsCdCM004101: 통제유형 - 통제성[CM00410101], 비통제성[CM00410102]
	 ************************************************************************************/

    /************************************************************************************
     *  [1] 전역변수
     ************************************************************************************/
	var svcId;		//callback 처리용 서비스ID
	scwin.selectStndItemiCd = "";	//TREE에서 선택된 표준내역코드
	scwin.topLvCd = "ROOT";	//최상위레벨 표준내역코드

    /************************************************************************************
     *  [2] onpageload(초기화포함)
     ************************************************************************************/
	scwin.onpageload = function() {

	};

	scwin.onpageunload = function() {

	};

    /** 화면초기화 **/
    scwin.fnInitial = function() {
        //공통코드 : 통제유형
		com.selCdList("sbxCtrlTypeCd", "CM004101", "Y");
        //공통코드 : 비목
		com.selCdListVrbl("sbxItexpGb", {"upperCd": "CM004100","setCd": "charValN1", "setNm" : "cdNm"});

        grpStndCdDtl.init();						//표준내역TAB - 표준내역 상세정보(내역) 초기화

		//선택된 표준내역코드 초기화
		scwin.selectStndItemiCd = "";

		dsSearchParamMap.set( "tabGubun" , "tabs2" );//표준내역TAB
		dsSearchParamMap.set( "stndItemiCd" , "" );//표준내역코드 검색조건 초기화

		//표준내역TAB - TREE 조회
		scwin.fnTreeList();

		//표준내역TAB - 표준내역 상세정보 - 버튼 비활성화
		scwin.fnStndBtnDtlDisabled(true);

        //UI팀용 함수 호출
        com.commonUI();
    };

    /************************************************************************************
     *  [3] 사용자 정의함수 함수
     ************************************************************************************/
	//트리조회
	//표준공종TAB과 표준내역TAB에서 동일하게 사용함.
	//dsSearchParamMap.tabGubun의 구분값으로 Business logic에서 분기처리함.
	scwin.fnTreeList = function(){
		svcId = "selectTcmStndJobtypCdTreeDtlList";
        ajaxLib.ajax("/cm/cm0015/CM00150000U/selectTcmStndJobtypCdTreeList", {
        	mode : "asynchronous"
        	, method : "POST"
            , requestData : dsSearchParamMap.getJSON()
            , callback : "fnCallback"
            , errCallback : "fnErrCallBack"
        });
	};

	//표준내역TAB - 표준내역 상세정보 - 버튼 활성/비활성화
	scwin.fnStndBtnDtlDisabled = function(flag){
		btnNew.setDisabled(flag);//표준내역TAB - 표준내역 상세정보 - 내역추가
		btnSaveDtl.setDisabled(flag);//표준내역TAB - 표준내역 상세정보 - 저장
		btnDel.setDisabled(flag);//표준내역TAB - 표준내역 상세정보 - 삭제
	}

	//표준내역TAB - 표준내역 상세정보
	scwin.fnStndJobtypCdDtlList = function(){
		svcId = "selectTcmStndJobtypCd";
        ajaxLib.ajax("/cm/cm0015/CM00150010T/selectTcmStndJobtypCd", {
        	mode : "asynchronous"
        	, method : "POST"
            , requestData : dsSearchParamMap.getJSON()
            , callback : "fnCallback"
            , errCallback : "fnErrCallBack"
        });
	}

    /************************************************************************************
     *  [4] Callback 함수
     ************************************************************************************/
	/** 오류시 처리 **/
	scwin.fnErrCallBack = function(result, e) {

	};

    /** 정상시 후처리 **/
    scwin.fnCallback = function(result, e) {
    	switch(svcId) {
			//표준내역 TAB - TREE 조회
			case "selectTcmStndJobtypCdTreeDtlList" :
				dsTcmStndJobtypCdTreeDtlList.removeAll();
    			dsTcmStndJobtypCdTreeDtlList.setJSON(result.data.dsTcmStndJobtypCdTreeList);

    			//저장, 수정일 경우 TREE 조회 후 해당 내역으로 focus 처리
    			//삭제일 경우 최상위 레벨로 focus처리
    			trvCodeDtlList.findNodeByValue( scwin.selectStndItemiCd , true );

    			if(scwin.selectStndItemiCd == scwin.topLvCd){//표준내역 상세정보 - 삭제
    				grpStndCdDtl.init();//상세정보(내역) 초기화
    				scwin.fnStndBtnDtlDisabled(true);//표준내역TAB - 표준내역 상세정보 - 버튼 비활성화
					dsSearchParamMap.set( "stndItemiCd" , "" );//검색조건 초기화
    			}else{//표준내역 상세정보 - 저장
	    			dsSearchParamMap.set( "stndItemiCd" , scwin.selectStndItemiCd );
	    			scwin.fnStndJobtypCdDtlList();
    			}

				break;
			//표준내역 TAB - 표준내역 상세정보 조회
			case "selectTcmStndJobtypCd" :
				dsTcmStndJobtypCdDtlMap.setEmptyValue();
    			dsTcmStndJobtypCdDtlMap.setJSON(result.data.dsTcmStndJobtypCdDtlMap);    	    		
				break;
			//표준내역 TAB - 표준내역 상세정보 - 저장
			case "saveTcmStndJobtypCdDtl" :
				com.showMessage("CFA0012");//저장되었습니다.
				scwin.selectStndItemiCd = result.data.stndItemiCd;
                ibxSortOdr.setReadOnly( false );//정렬순서
    			scwin.fnTreeList();
				break;
			//표준내역 TAB - 표준내역 상세정보 - 삭제
			case "deleteTcmStndJobtypCdDtl" :
				com.showMessage("EMA0006");//삭제되었습니다.
				scwin.selectStndItemiCd = result.data.stndItemiCd;
    			scwin.fnTreeList();
				break;
			//표준내역 TAB - 회계계정 - 저장
			case "saveTcmStndJobtypAcctMapp" :
				com.showMessage("CFA0012");//저장되었습니다.
				break;
			default :
				break;
    	}
    };

    /************************************************************************************
     *  [5] Event 함수
     ************************************************************************************/
	//표준내역TAB - 트리 클릭 이벤트
	scwin.trvCodeDtlList_onlabelclick = function(value,nodeElement,index) {
		var stndItemiCd = dsTcmStndJobtypCdTreeDtlList.getCellData( index-1 , "stndItemiCd" );//내역코드
		var itemiYn = dsTcmStndJobtypCdTreeDtlList.getCellData( index-1 , "itemiYn" );//내역여부
		var itemiYnNext = dsTcmStndJobtypCdTreeDtlList.getCellData( index-1 , "itemiYnNext" );//하위레벨 내역여부
		var upperStndItemiCdNext = dsTcmStndJobtypCdTreeDtlList.getCellData( index-1 , "upperStndItemiCdNext" );//하위레벨의 상위_표준내역코드

		if(stndItemiCd == upperStndItemiCdNext && itemiYnNext == "N"){//하위레벨에 자식공종이 존재하면 EDIT 불가능.
			scwin.fnStndBtnDtlDisabled(true);//표준내역TAB - 표준내역 상세정보 - 버튼 비활성화
		}else if( (stndItemiCd != upperStndItemiCdNext && itemiYnNext == "N")	//하위레벨에 자식공종이 존재하지 않으면 EDIT 가능.
					|| (stndItemiCd == upperStndItemiCdNext && itemiYnNext == "Y")	//하위레벨에 내역이 존재하면 EDIT 가능
					|| (itemiYn == "Y")	//표준내역일 경우 EDIT 가능
					){
			scwin.fnStndBtnDtlDisabled(false);//표준내역TAB - 표준내역 상세정보 - 버튼 활성화
		}

		if(itemiYn == "Y"){//표준내역일 경우.
			dsSearchParamMap.set( "stndItemiCd" , stndItemiCd );
			scwin.fnStndJobtypCdDtlList();//표준내역 상세정보
            ibxSortOdr.setReadOnly( false );//정렬순서
		}else{
			grpStndCdDtl.init();//표준내역TAB - 상세정보(내역) 초기화			
		}
	};

    //표준내역TAB - 표준내역 상세정보 - 내역추가
    scwin.btnNew_onclick = function() {
        grpStndCdDtl.init();//표준내역TAB - 표준내역 상세정보(내역) 초기화

        var selectIdx = trvCodeDtlList.getSelectedIndex();
        var itemiYn = dsTcmStndJobtypCdTreeDtlList.getCellData( selectIdx-1 , "itemiYn" );//내역여부
        var stndItemiCd = dsTcmStndJobtypCdTreeDtlList.getCellData( selectIdx-1 , "stndItemiCd" );//표준내역코드
        var stndItemiNm = dsTcmStndJobtypCdTreeDtlList.getCellData( selectIdx-1 , "stndItemiNm" );//표준내역명
        var stndItemiLv = dsTcmStndJobtypCdTreeDtlList.getCellData( selectIdx-1 , "stndItemiLv" );//표준내역레벨

        if(itemiYn == "Y"){//내역을 클릭했을 경우는 상위 코드를 셋팅.
            stndItemiCd = dsTcmStndJobtypCdTreeDtlList.getCellData( selectIdx-1 , "upperLvStndItemiCd" );//상위레벨 표준내역코드
            stndItemiNm = dsTcmStndJobtypCdTreeDtlList.getCellData( selectIdx-1 , "upperLvStndItemNm" );//상위레벨 표준내역명
            dsTcmStndJobtypCdDtlMap.set( "stndItemiLv" , stndItemiLv );//표준내역레벨
        }else{//공종일경우.
            dsTcmStndJobtypCdDtlMap.set( "stndItemiLv" , WebSquare.util.parseInt( stndItemiLv , 4 )+1 );//표준내역레벨
        }

        dsTcmStndJobtypCdDtlMap.set( "itemiYn" , "Y" );//내역여부

        ibxUpperStndItemiCd.setValue( stndItemiCd );//상위공종코드
        ibxUpperStndItemiNm.setValue( stndItemiNm );//상위공종명
        ibxSortOdr.setReadOnly( true );//정렬순서
        sbxItexpGb.setValue( "O" );//비목: 기본값 (경비)
    };

    //표준내역TAB - 표준내역 상세정보 - 저장
    scwin.btnSaveDtl_onclick = function() {

        if(ibxUpperStndItemiCd.getValue() == ""){
        	com.showMessage("CSA0008", "상위공종코드", "", "", ibxUpperStndItemiCd.id);//$1이(가) 존재하지 않습니다.
        	return;
        }else if(ibxStndItemiNm.getValue() == ""){
            com.showMessage("SAA0043", "내역명", "", "", ibxStndItemiNm.id);//$1은(는) 필수입력사항입니다.
            return;
        }else if(ibxStndItemiUnit.getValue() == ""){
        	com.showMessage("SAA0043", "단위", "", "", ibxStndItemiUnit.id);//$1은(는) 필수입력사항입니다.
            return;
        }else if(sbxItexpGb.getValue() == ""){
        	com.showMessage("SAA0043", "비목", "", "", sbxItexpGb.id);//$1은(는) 필수입력사항입니다.
            return;
