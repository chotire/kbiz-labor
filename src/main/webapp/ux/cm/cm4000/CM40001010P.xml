    /************************************************************************************
     *  [1] onpageload
     ************************************************************************************/
    scwin.onpageload = function() {
     	//화면초기화
        scwin.fnInitial();

    };

    /** 화면초기화 **/
    scwin.fnInitial = function() {
  
        //파리미터
        paramKeyWkrNo = JSON.parse($p.getPopupParam()).data.paramKeyWkrNo;

        if ( paramKeyWkrNo != ""  ){
            dsParamMap.set( "wkrNo" , paramKeyWkrNo );
            scwin.fnSearchList();
        }
        //UI팀용 함수 호출
        scwin.commonUI();
    };

    /************************************************************************************
     *  [2] 전역변수
     ************************************************************************************/
    var svcId = "";

    /************************************************************************************
     *  [3] Event 함수
     ************************************************************************************/

    /**************************************************************************
     *  [4] Callback
     **************************************************************************/

    // 결과 후처리
    scwin.fnCallback = function(result, e) {

        switch (svcId) {
            case "selectTcmWkrHistoryList" :		//조회

                dsTcmWkr.setJSON(result.data.tcmWkrHistoryList);            //현장 이력
                dsTcmHlthExmn.setJSON(result.data.tcmHlthExmnHistoryList);  //건강검진 이력
                dsTcmWkrMstr.setJSON(result.data.tcmWkrMstr);               //건강검진 마스터

                var nRowCount = grdTcmWkr.getTotalRow();
		        if(  nRowCount > 0 ){
		            grdTcmWkr.setFocusedCell( 0 , "sitenm" , false);
		        }

                break;
        }
    };

    // 에러 후처리
    scwin.fnErrCallBack = function(e) {
        switch (svcId) {
            case "selectTcmWkrHistoryList" :     	//조회
                console.log('조회오류: ' + e);
                break;
        }
    }

    /**************************************************************************
     *  [5] 사용자정의 함수
     **************************************************************************/
    // 조회 List
    scwin.fnSearchList = function() {

        svcId = "selectTcmWkrHistoryList";

        ajaxLib.ajax("/cm/cm4000/CM40001010P/selectTcmWkrHistoryList", {
            mode : "synchronous",
            method : "POST",
            requestData : dsParamMap.getJSON(),
            callback : "fnCallback",
            errCallback : "fnErrCallBack"
        });

    };
]]></script>


