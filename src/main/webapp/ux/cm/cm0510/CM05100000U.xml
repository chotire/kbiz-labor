 **************************************************************************/
    scwin.onpageload = function() {   


        //추가 코드값 맵핑 호출
        scwin.fnInitial();
    };

    /** 화면초기화 **/
    scwin.fnInitial = function() {

        com.selCdListVrbl( "sbxBzpjGbCd" , { /*사업구분*/
            "upperCd": "CF074"
       });
       
  /*     com.selCdListVrbl( "sbxSpvsYnCd" , { 
            "upperCd": "SA094"
       });*/ // 참여구분 방식 아래로 변경 초기값 설정때문
     //   dsSearchMap.set("spvsYnCd","SA09401"); // 진행상태 초기값 : 진행
        sbxSpvsYnCd.setValue( "SA09401" );
        
        dsSearchMap.set("actbgtConfmStusCd", "CM1000052"); // 진행상태 초기값 : 진행        
        com.commonUI();
    };

/**************************************************************************
 * [2] Event 정의
 **************************************************************************/

    //조회 클릭
	scwin.btnFind_onclick = function() {
	
		dsSearchMap.set("frStwkDt", com.getUdcValue( "udcStdDtmStwkDt" , "frDate" ));
		dsSearchMap.set("toStwkDt", com.getUdcValue( "udcStdDtmStwkDt" , "toDate" ));
		dsSearchMap.set("frComplDt", com.getUdcValue( "udcStdDtmComplDt" , "frDate" ));
		dsSearchMap.set("toComplDt", com.getUdcValue( "udcStdDtmComplDt" , "toDate" ));
		dsSearchMap.set("frStwkDocDt", com.getUdcValue( "udcStwkDocDt" , "frDate" ));
		dsSearchMap.set("toStwkDocDt", com.getUdcValue( "udcStwkDocDt" , "toDate" ));
		dsSearchMap.set("spvsYnCd", sbxSpvsYnCd.getValue());
		
        ajaxLib.ajax("/cm/cm0510/CM05100000U/selectTcmSiteMonitorList", {
            method: "POST",
            requestData: dsSearchMap.getJSON(),
            callback: function(result, e) {
                dsTcmSiteMonitorList.setJSON(result.data);
                com.setUdcValue("dsTcmSiteMonitorListCount", "udcListCount", grdTcmSiteMonitorList.getRowCount());
            },
            errCallback: function(result, e) {
                com.showMessage("CFE0112"); //작업 중 에러가 발생하였습니다. 관리자에게 문의하십시오
            }
        });
        
	};
	
	// 현장명 클릭
	scwin.grdTcmSiteMonitorList_oncellclick = function(row,col) {

        var sColunmName = grdTcmSiteMonitorList.getColumnID( col );
        sessionStorage.cmSiteCd = grdTcmSiteMonitorList.getCellData( row , "siteCd" )
        sessionStorage.cmSiteNm = grdTcmSiteMonitorList.getCellData( row , "siteNm" );
        if (sColunmName == "siteNm") { 

            com.detailOpen("공사개요모니터링(공사개요)", "CM05000000U", "/ux/cm/cm0500/CM05000000U.xml", "2", {

            });
        }

    };
	
	//조회조건 현장 엔터
	scwin.sbxSiteNm_onkeydown = function(e) {
        if(e.key=="Enter"){
            scwin.btnFind_onclick();
        }
	};


    //엑셀다운 클릭
	scwin.btnExcelDwn_onclick = function() {
        grdTcmSiteMonitorList.advancedExcelDownload({
            fileName:"공사개요Monitoring.xlsx",
            useSubTotal : true,             // Sub-Total 표시 
            headerFontColor : "#FFFFFF",    // Header Font Color
            headerColor : "#D9D9D9",        // Header Back Color
            subTotalColor : "#FCE4D6",      // SubTotal Back Color
            footerColor : "#BDD7EE"         // Footer Color
        });
	};
	]]></script>


