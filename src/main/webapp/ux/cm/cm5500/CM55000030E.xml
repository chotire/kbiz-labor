/************************************************************************************ 
 *  [1] onpageload
************************************************************************************/   
	scwin.onpageload = function() {    
	   scwin.fnInitial();	
	};
	
    /** 화면초기화 **/
    scwin.fnInitial = function() {           
        //UI팀용 함수 호출
        com.commonUI();        
        var dsSrch = com.getAprvParam("dsSrch"); // 부모창의 데이터맵을 읽어온다.
        dsSearchMap.setJSON( dsSrch.getJSON() );                     
        scwin.fnSearch();
        $("body").css("overflow","scroll"); // 링크 팝업시 화면 스크롤 생성
               
    };        
      
    /************************************************************************************
     *  [2] 사용자 정의함수 함수 
     ************************************************************************************/    
   
    scwin.fnSearch = function() {     
        ajaxLib.ajax("/cm/cm1505/CM15050025U/selectTcmSite", {
            method: "POST",
            requestData: dsSearchMap.getJSON(),
            callback: function(result, e){
                dsTcmSiteMap.setJSON(result.data.tcmSite);                
            },
            errCallback: function(result, e) {       
                console.log(result);
            }    
        });
    };
 ]]></script>		
