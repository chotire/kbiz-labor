 * 변경일자     수정자   내 용 
 * ======================================== 
 * 2019.03.18   tpcsy	 최초 생성
 *
 **************************************************************************/
 
/**************************************************************************
 * DataCollection description
 *
 *	dsSearchMap : 검색조건 - 프로젝트ID (목록에서 전달받은 PARAMETER)
 *  dsTcmSitedRptsMap : 현설청취보고서 Map
 **************************************************************************/
 
  
 /**************************************************************************
 * [1] 전역변수 선언
 **************************************************************************/
    var svcId = "";

			
/**************************************************************************
 * [2] onpageload(초기화포함)
 **************************************************************************/
    scwin.onpageload = function() {
        //추가 코드값 맵핑 호출
        scwin.fnInitial();
    };

    /** 화면초기화 **/
    scwin.fnInitial = function() {
    
		var dsSrch = com.getAprvParam("dsSrch"); // 부모창의 데이터맵을 읽어온다.
		dsSearchMap.setJSON( dsSrch.getJSON() );

		//공통코드 : 낙찰자선정코드[ES005]
		//com.selCdList("sbxSbidrSelnCd", "ES005", "Y");

		//공통코드 : 입찰절차
		//com.selCdList("sbxBzdpTndrProcCd", "ES006", "Y");
		
		//공통코드 : 포함공사값
		com.selCdList("cbxIncluConstVal", "ES007", "Y");

		//화면로딩 시 자동 조회
		scwin.fnTcmSitedRpts();
		
        com.commonUI();
        //grpSearch.init();                   //조회그룹 초기화        
    };
    
 /**************************************************************************
 * [3] 사용자정의 함수
 **************************************************************************/

  	/**************************************************************************
	 * 함 수 명: scwin.fnTcmSitedRpts
	 * 기 능: 현설청취보고서 & 현설참가업체 LIST & 첨부파일 LIST SELECT
	 **************************************************************************/
     scwin.fnTcmSitedRpts = function() {
		svcId = "selectTcmSitedRpts";
        ajaxLib.ajax("/cm/cm0000/CM00000505U/selectTcmSitedRpts", {            
            mode : "asynchronous",
            method : "POST",
            requestData : dsSearchMap.getJSON(),
            callback : "fnCallback",
            errCallback : "fnErrCallback"
        }); 
    };

  	/**************************************************************************
	 * 함 수 명: scwin.fnSelectAfter
	 * 기 능: 조회 후 후처리
	 **************************************************************************/
	scwin.fnSelectAfter = function() {
		// 포함공사 CHECK 처리		
		var sIncluConstVal = dsTcmSitedRptsMap.get( "incluConstVal" );
		var sChkSplit = sIncluConstVal.replace(/,/gi, " ");	// setValue 사용 시 공백으로 대체함
		cbxIncluConstVal.setValue( sChkSplit );
		
		//TEXTAREA height resize
		scwin.fnTextAreaReSize(txaConstOutlCn.id);//내용
		scwin.fnTextAreaReSize(txaExclConstCn.id);//제외공사
		scwin.fnTextAreaReSize(txaPayBdmtCn.id);//지급자재
		scwin.fnTextAreaReSize(txaSpcmtCn.id);//특기사항
		scwin.fnTextAreaReSize(txaSitedPrtcpCompEtcCn.id);//현설참가(기타)
	};    
    
  	/**************************************************************************
	 * 함 수 명: scwin.fnTextAreaReSize
	 * 기 능: TEXTAREA height resize
	 **************************************************************************/
    scwin.fnTextAreaReSize = function(id){
		var textEle = $("#"+id);
		textEle[0].style.height = 'auto';
		var textEleHeight = textEle.prop('scrollHeight');
		textEle.css('height', textEleHeight+12);    
    };
    
/**************************************************************************
 * [4] Callback
 **************************************************************************/
    scwin.fnCallback = function(result, e) {
        switch(svcId) {
            case "selectTcmSitedRpts" :	// 초기 화면 로딩 시
                dsTcmSitedRptsMap.setJSON(result.data.tcmSitedRpts);
                scwin.fnSelectAfter();
                break; 
        }       
    }    
    
    scwin.fnErrCallback = function(result, e) {
       console.log(result);
    }     

    
 /**************************************************************************
 * [5] Event 정의
 **************************************************************************/            
]]></script>

