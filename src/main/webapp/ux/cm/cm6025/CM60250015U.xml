	/**************************************************************************
	 * [1] 전역변수 선언
	 **************************************************************************/
 
	/**************************************************************************
	 * [2] onpageload(초기화포함)
	 **************************************************************************/
    scwin.onpageload = function() {   
        //추가 코드값 맵핑 호출
        scwin.fnInitial();
    };

    /** 화면초기화 **/
    scwin.fnInitial = function() {
		cmCom.setSite(wfmKeySiteInfo.getWindow());	               //세션의 현장코드를 셋팅
        dsSearchMap.set("workYm",$p.getCurrentServerDate( "yyyyMM" ));
 		grdList.initGrid();	                                       //전도금사용내역목록 GRID 초기화
        com.commonUI();		                                       //UI팀용 함수 호출
    };

	/**************************************************************************
	 * [3] 사용자정의 함수
	 **************************************************************************/
  	/**************************************************************************
	 * 함 수 명: scwin.fnSearchList
	 * 기 능: 기타 노무비 정보 조회
	 **************************************************************************/
	scwin.fnSearchList = function() {
		ajaxLib.ajax("/cm/cm6025/CM60250000U/selectTcmLabrLbatnEtcList",{
			method : "POST",
			requestData : dsSearchMap.getJSON(),
			callback : function(result, e){      
				dsList.setJSON(result.data.dsList); 
				com.setUdcValue("dsListCount", "udcListCount", dsList.getRowCount());
                
                for(let i = 0; i < dsList.getRowCount(); i++)
                {
                    let sLbatnQty = dsList.getCellData( i ,  "lbatnQty");
                    if(sLbatnQty.indexOf(".") == -1)
                    {
                        dsList.setCellData( i , "lbatnQty" , sLbatnQty + ".0");
                    } 
                }
                
                dsList.initRowStatus();     
                 	
                grdList.setFocusedCell( 0 , 1 , false );			 
			},
			 errCallback: function(result, e) {
                console.log(result);
            }
		});
    };
    
	/**************************************************************************
	 * [4] Callback
	 **************************************************************************/


	/************************************************************************************
	 *  [5] Event 함수
	 ************************************************************************************/
    /**************************************************************************
     * Event: scwin.WFrameSiteInfo_change_callback
     * 기 능: 현장코드 변경 이벤트
     **************************************************************************/
	scwin.WFrameSiteInfo_change_callback = function() {
		dsSearchMap.set( "siteCd" , wfmKeySiteInfo.getWindow().ibxKeySiteCd.getValue());
		dsSearchMap.set( "siteNm" , wfmKeySiteInfo.getWindow().ibxKeySiteNm.getValue());
        
        if(!com.isEmpty(dsSearchMap.get("workYm")))
        {
            scwin.btnFind_onclick();	
        }
	};

    /**************************************************************************
     * Event: scwin.btnFind_onclick
     * 기 능: 조회 버튼 Click 이벤트
     **************************************************************************/
	scwin.btnFind_onclick = function() {
	    if (com.isEmpty(dsSearchMap.get("siteCd"))){
	        com.showMessage("CMW0025", "현장");      //$1은(는) 필수입력입니다.
	        return false;
	    }
	    
        if (com.isEmpty(dsSearchMap.get("workYm"))){
	        com.showMessage("CMW0025", "출역년월");  ////$1은(는) 필수입력입니다.
	        return false;
	    }
        
        dsSearchMap.set( "lbatnDt" , dsSearchMap.get("workYm") + "01"); //조회년월 세팅
        
		scwin.fnSearchList();
	};
	
	/**************************************************************************
     * 함 수 명: scwin.btnExcelDwn_onclick
     * 기 능: 엑셀다운버튼 Click 이벤트
     **************************************************************************/
    scwin.btnExcelDwn_onclick = function() {
        let result = [];
        //result.push(0);//checkbox 제외
       
        let totalCol = grdList.getTotalCol();
       
        for(let ex = 0; ex < totalCol; ex++) {
            if (!grdList.getColumnVisible(ex)) {
                result.push(ex);
            }
        }
        
        let excelOpt = {
             fileName: "기타_노무비_등록.xlsx"        //또는 xlsx 확장자 사용
            ,removeColumns: result.join(",")
            ,useSubTotal: true                      //colMerge된 컬럼을 Merge해서 출력 할 지 여부
            ,colMergeTextAlign: "center"            //colMerge된 컬럼의 textAlign설정 (bottom, center, top 설정)
            ,useStyle: "true"
            ,indent: 1
            ,useDataFormat : true
        };  //excel download 옵션
        
        grdList.advancedExcelDownload( excelOpt );       
    };

    /*그리드 행삭제*/
	scwin.btnDelRow_onclick = function() {
        if (dsList.getRowCount() == 0) {
            com.showMessage("SMA0031", "삭제"); //$1할 행이 선택되지 않았습니다.
            return;
        }
        
        dsList.removeRow(dsList.getRowPosition());
	};
	
    /*그리드 국민연금, 고용보험 건강보험 변경 시 */
	scwin.dsList_oncelldatachange = function(info) {
        let rowIndex       = info.rowIndex;
        // 지급액(지급총액, 보너스, 식대, 기타지급) 
        let nPayAmt        = dsList.getCellData(rowIndex,"payAmt");         //지급액
        let nBonusAmt      = dsList.getCellData(rowIndex,"bonsAmt");        //보너스
        let nMealAmt       = dsList.getCellData(rowIndex,"mealAmt");        //식비
        let nEtcAmt        = dsList.getCellData(rowIndex,"etcPayAmt");      //기타지급
        // 공제액(갑근세, 주민세, 국민연금, 고용보험, 건강보험료, 기타공제액)
        let nIctxAmt       = dsList.getCellData(rowIndex,"ictxAmt");        //소득세금액
        let nRstxAmt       = dsList.getCellData(rowIndex,"rstxAmt");        //주민세세금액
        let nNatnpPnsnAmt  = dsList.getCellData(rowIndex,"natnpPnsnAmt");   //국민연금
        let nEplmtInfeeAmt = dsList.getCellData(rowIndex,"eplmtInfeeAmt");  //고용보험
        let nHlthInfeeAmt  = dsList.getCellData(rowIndex,"hlthInfeeAmt");   //건강보험료
        let nEtcDdctAmt    = dsList.getCellData(rowIndex,"etcDdctAmt");     //기타공제액
        let nNetpayAmt     = (nPayAmt+nBonusAmt+nMealAmt+nEtcAmt) - (nIctxAmt+nRstxAmt+nNatnpPnsnAmt+nEplmtInfeeAmt+nHlthInfeeAmt+nEtcDdctAmt);
        
        dsList.setCellData(rowIndex,"netpayAmt",nNetpayAmt);
	};
	
    //저장 
	scwin.btnSave_onclick = function() {
		scwin.fnSave("");
	};
    
    //기타 노무비 저장
    scwin.fnSave = function(val) {
        let saveList = dsList.getModifiedJSON();

        // 수정여부 체크
        if (saveList.length == 0) {
            com.showMessage("SCA0007", "", "", ""); // 저장(변경된)할 목록이 없습니다.
            return;
        }
        
        if (confirm("저장 하시겠습니까?")) {
            ajaxLib.ajax("/cm/cm6025/CM60250000U/insertTcmLabrLbatnEtcList",{
                    method : "POST",
                    requestData : {
                            tcmLabrLbatnEtcList : dsList.getModifiedJSON()
                    },
                    callback : function(result, e){      
                            com.showMessage("CFA0012");//저장되었습니다.
                            scwin.btnFind_onclick();
                    },
                     errCallback: function(result, e) {
                        com.showMessage("CFE0200","기타 노무비 등록");// $1 처리시 오류가 발생하였습니다.
                    }
            });                    
        }   
    };    
	
    //공제계산
	scwin.btnCal_onclick = function() {
        if (confirm("공제계산 하시겠습니까?")) {
            ajaxLib.ajax("/cm/cm6025/CM60250000U/insertProcCalTcmLabrLbatnEtcList",{
                    method : "POST",
                    requestData : {
                            tcmLabrLbatnEtcList : dsList.getAllJSON()
                    },
                    callback : function(result, e){      
                            if (result.data.R_FLAG == "S"){
                                com.showMessage("SCN0002", "공제계산"); //$1 되었습니다.
                                scwin.fnSearchList();
                            } else {                            
                                com.showMessage("CFW0010", result.data.R_MSG);  //에러메세지.        
                            }
                    },
                     errCallback: function(result, e) {
                        com.showMessage("CFE0200","기타 노무비 등록");// $1 처리시 오류가 발생하였습니다.
                    }
            });                    
        }   		
	};
	
    //년월 변경 시 이벤트
	scwin.calYearMonth_onchange = function() {
		scwin.btnFind_onclick();
	};
	]]></script>
