		<script type="javascript" lazy="false"><![CDATA[
/**************************************************************************
 * [1] onpageload(초기화포함)
 **************************************************************************/
    scwin.onpageload = function() {

        //세션의 현장코드를 셋팅
        cmCom.setSite(wfmKeySiteInfo.getWindow());

        com.selCdListVrbl("sbxActbgtChgTypeCd", {
            upperCd: "CM004001",
            cd: "'CM0040016','CM0040013','CM0040014','CM0040017'"
        });
        
        com.selCdListVrbl("sbxFnncConfStusCd", {
                upperCd: "CM004008"
            },
            function(){
                sbxFnncConfStusCd.addItem("CM004008", "결재중", 0);

                dsSearchMap.setJSON(com.getAllParam());
                wfmKeySiteInfo.getWindow().ibxKeySiteCd.setValue(dsSearchMap.get("searchSiteCd"));
                wfmKeySiteInfo.getWindow().ibxKeySiteNm.setValue(dsSearchMap.get("searchSiteNm"));

                //메뉴에서 화면 호출한 경우 목록버튼 hide
                if (dsSearchMap.get("returnUrl") == ""){
                    //세션의 현장코드를 셋팅
                    cmCom.setSite(wfmKeySiteInfo.getWindow());
                    btnList.hide();
                }
                
                //구분: 실행예산 구분 셋팅
                scwin.fnSelectBgtGbCd(dsSearchMap.get("searchSiteCd"));

                //목록 버튼으로 넘어온 경우 자동조회
                if (dsSearchMap.get("listSearchYn") == "Y"){
                    scwin.btnFind_onclick();
                }
            }
        );

    };

/************************************************************************************
 *  [2] 사용자 함수
 ************************************************************************************/
    //현장의 실행예산구분 값 조회('1': 가실행예산, '2' : 실행예산)
    scwin.fnSelectBgtGbCd = function(siteCd) {
        setTimeout(null,0);
        ajaxLib.ajax("/cm/cm1510/CM15100500/selectBgtGbCd", {
            method: "POST",
            requestData: dsSearchMap.getJSON(),
            callback: function(result, e){
                
                console.log(result.data);
                sbxBgtGbCd.setValue(result.data.bgtGbCd);
                scwin.btnFind_onclick();
            },
            errCallback: function(result, e) {
                com.showMessage("CFE0112"); //작업 중 에러가 발생하였습니다. 관리자에게 문의하십시오
            }
        });

    };
/************************************************************************************
 *  [3] 이벤트
 ************************************************************************************/

    //현장코드 변경 이벤트
    scwin.WFrameSiteInfo_change_callback = function(){
        
        dsSearchMap.set("searchSiteCd", wfmKeySiteInfo.getWindow().ibxKeySiteCd.getValue());  //현장 코드
        dsTcmActbgtHistAcumList.removeAll();
        //구분: 실행예산 구분 셋팅
        scwin.fnSelectBgtGbCd(dsSearchMap.get("searchSiteCd"));

    };

    // 조회 클릭
    scwin.btnFind_onclick = function() {
        dsSearchMap.set("searchSiteCd", wfmKeySiteInfo.getWindow().ibxKeySiteCd.getValue());
        dsSearchMap.set("searchSiteNm", wfmKeySiteInfo.getWindow().ibxKeySiteNm.getValue());

        if (dsSearchMap.get("searchSiteCd") ==""){
            com.showMessage("SAA0031", "현장");     //을(를) 선택하십시오.
            return false;
        }

        ajaxLib.ajax("/cm/cm1510/CM15100500/selectTcmActbgtHistAcumList", {
            method: "POST",
            requestData: dsSearchMap.getJSON(),
            callback: function(result, e){
                dsTcmActbgtHistAcumList.setJSON(result.data);
                com.setUdcValue("dsTcmActbgtHistAcumListCount", "udcListCount", dsTcmActbgtHistAcumList.getColumnDistinctDataArray("chgDgr").length);
            },
            errCallback: function(result, e) {
                com.showMessage("CFE0112"); //작업 중 에러가 발생하였습니다. 관리자에게 문의하십시오
            }
        });
	};

    //엑셀다운 클릭
	scwin.btnExcelDwn_onclick = function() {
        grdTcmActbgtHistAcumList.advancedExcelDownload({
            fileName:"실행예산변경이력.xlsx",
            useSubTotal: true,              //colMerge된 컬럼을 Merge해서 출력 할 지 여부
            colMergeTextAlign: "center",    //colMerge된 컬럼의 textAlign설정 (bottom, center, top 설정)
            colMerge : true,
            headerFontColor : "#FFFFFF",    // Header Font Color
            headerColor : "#D9D9D9",        // Header Back Color
            subTotalColor : "#FCE4D6",      // SubTotal Back Color
            footerColor : "#BDD7EE"         // Footer Color
        });
	};

    //그리드 셀 클릭
	scwin.grdTcmActbgtHistAcumList_oncellclick = function(row,col) {
        var sColunmName = grdTcmActbgtHistAcumList.getColumnID(col);
        if (sColunmName == "chgDgrNm") {
            dsSearchMap.setJSON({
                siteCd: wfmKeySiteInfo.getWindow().ibxKeySiteCd.getValue(),
                siteNm: wfmKeySiteInfo.getWindow().ibxKeySiteNm.getValue(),
                bgtGbCd: dsTcmActbgtHistAcumList.getCellData(row, "bgtGbCd"),
                bgtGbNm: dsTcmActbgtHistAcumList.getCellData(row, "bgtGbNm"),
                actbgtChgTypeCd: dsTcmActbgtHistAcumList.getCellData(row, "actbgtChgTypeCd"),
                actbgtChgTypeNm: dsTcmActbgtHistAcumList.getCellData(row, "actbgtChgTypeNm"),
                chgDgr: dsTcmActbgtHistAcumList.getCellData(row, "chgDgr") + "",
                chgDgrNm: dsTcmActbgtHistAcumList.getCellData(row, "chgDgrNm"),
                fnncConfStusCd: dsTcmActbgtHistAcumList.getCellData(row, "fnncConfStusCd"),  //재무확인상태코드
                returnScrNm: "실행예산 변경 이력관리",
                returnUrl: "/ux/cm/cm1510/CM15100500U.xml"
            });

            com.detailOpen("실행예산변경이력관리(상세)", "CM15100505U", "/ux/cm/cm1510/CM15100505U.xml", "1", dsSearchMap.getJSON());

        } else if (sColunmName == "aproDt"){    //결재현황
            //com.aprvDocViewPopup(dsTcmActbgtHistAcumList.getCellData(row, "gwDocNo"));
        }
    };

	//목록 클릭
	scwin.btnList_onclick = function() {
        dsSearchMap.set("listSearchYn", "Y");
        com.detailOpen(dsSearchMap.get("returnScrNm"), "CM1510", dsSearchMap.get("returnUrl"), "1", dsSearchMap.getJSON());
	};
	]]></script>


