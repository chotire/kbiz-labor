/**************************************************************************
 * [1] 전역변수 선언
 **************************************************************************/
	var pMap;

/**************************************************************************
 * [2] onpageload(초기화포함)
 **************************************************************************/
	scwin.onpageload = function() {
        //세션의 현장코드를 셋팅
        cmCom.setSite(wfmKeySiteInfo.getWindow());
		pMap = $p.getFrame().getObj("dsSearchMap");
//        com.searchSwich();  //검색조건3줄이상 열고닫기
//        com.autoHeight();   //그리드 채우기
	};

/************************************************************************************
 *  [3] Event 함수
 ************************************************************************************/
     //현장코드 변경 이벤트
    scwin.WFrameSiteInfo_change_callback = function(){
        dsSearchMap.set("siteCd", wfmKeySiteInfo.getWindow().ibxKeySiteCd.getValue());  //현장 코드
        scwin.btnFind_onclick();
    };

    //조회 클릭
    scwin.btnFind_onclick = function() {
        dsSearchMap.set("siteCd", wfmKeySiteInfo.getWindow().ibxKeySiteCd.getValue());  //현장 코드
        dsSearchMap.set("siteNm", wfmKeySiteInfo.getWindow().ibxKeySiteNm.getValue());  //현장 코드

        if (dsSearchMap.get("siteCd") ==""){
            com.showMessage("SAA0031", "현장");     //을(를) 선택하십시오.
            return false;
        }

        ajaxLib.ajax("/cm/cm1505/CM15050025U/selectTcmSite", {
            method: "POST",
            requestData: dsSearchMap.getJSON(),
            callback: function(result, e){
                dsTcmSiteMap.setJSON(result.data.tcmSite);
                dsTcmActbgtList.setJSON(result.data.selectTcmActbgtAdJustList);
            },
            errCallback: function(result, e) {
                com.showMessage("CFE0112"); //작업 중 에러가 발생하였습니다. 관리자에게 문의하십시오
            }
        });
    };

    //결재상신 클릭
    scwin.btnAprv_onclick = function() {

        if (dsTcmSiteMap.get("siteCd") == ""){
            com.showMessage("CMW0142"); //먼저 현장을 선택 후 조회하세요.
            return false;
        }

        ajaxLib.ajax("/cm/cm1505/CM15050025U/selectTcmSiteAdjCheck", {
            method: "POST",
            requestData: dsSearchMap.getJSON(),
            callback: function(result, e){

                if (result.data.selectActbgtChgTypeCdCnt.actbgtChgTypeCdCnt > 0){
                    com.showMessage("CMW0117");   //추초과 진행 중인 건이 존재합니다. 결재상신 할 수 없습니다.
                    return false;
                }
                if (result.data.selectActbgtChgTypeCdCnt.sitePrgsStusCd == "CM1000057"){
                    com.showMessage("CMW0151");   //이미 정산처리된 현장입니다. 결재상신 할 수 없습니다.
                    return false;
                }
                if (result.data.selectCheckCostDiff == "N"){
                    com.showMessage("CMW0071");   //공무재무원가 차이가 존재합니다. 공무재무시산표차이분석 화면에서 확인하십시요."
                    return false;
                }

                // 결재 후 처리 SP -- PCM_ACTBGTWORK_TO_ACTBGT
                //var sLinkUrl = "/ux/cm/cm1510/CM15100510T.xml&siteCd=" + dsSearchMap.get("siteCd")+ "&chgDgr="
                dsSearchMap.set("exctAproTtl", "[" + dsSearchMap.get("siteNm")+ "][실행예산정산]");
                dsSrch.setJSON(dsSearchMap.getJSON());                      //결재용 셋팅
                var paramObj = {
                    jobGbCd: "CM",                                         // 업무구분코드
                    importUrl: "/ux/cm/cm5500/CM55000030E.xml",            // 결재본문페이지
                    aprvTtl: dsSearchMap.get("exctAproTtl"),               // 결재제목
                    rvwrEmpno: "",                                         // 검토자_사번
                    fnlAprprEmpno: "",                                     // 최종승인자_사번
                    aprvPrcgNm: "",                                        // 후처리결재처리명
                    aprvPrcgEventNm: "/cm/cmif/CMIfTelepiaPostProcess/notifyAprvPcmSetActbgtDgrStatus",  // 후처리결재처리 이벤트명
                    aprvDocModiPsblYn: "Y",                                // 결재문서 수정불가기능 Y/N
                    aprlnModiPsblYn: "N",                                  // 결재선 수정불가기능 Y/N
                    aprvAtchModiPsblYn: "Y",                               // 결재첨부 수정불가기능 Y/N
                    aprvAflId: null                                          // 본문링크용 첨부파일 id (콤머로 구분해서 보내준다)
                    //popWidth: "1100",
                    //popHeight: "600"
                    //anchorUrl: "/ux/cm/cm1510/CM15100510T.xml&siteCd=000001&chgDgr=13",
                    //anchorUrlMsg: ">>실행예산 총괄표 조회"                 // 링크에 붙는 메시지
                };

                com.aprvSendEditor(paramObj); //의견달기호출
            },
            errCallback: function(result, e) {
                com.showMessage("CFE0112"); //작업 중 에러가 발생하였습니다. 관리자에게 문의하십시오
            }
        });

    };

/************************************************************************************
 *  [4] 사용자 정의함수 함수
 ************************************************************************************/
    //결재버튼 클릭 후 결재번호 리턴
    scwin.aprvPublicAction = function() {

        if (com.getAprvNo() > 0){
            dsSearchMap.set("aprvNo", com.getAprvNo());         //결재번호
            dsSearchMap.set("actbgtChgTypeCd", "CM004001A");    //정산
            btnAprv.setDisabled(true);
            ajaxLib.ajax("/cm/cm1510/CM15100000/approveTcmActbgtWork", {
                method: "post",
                requestData: dsSearchMap.getJSON(),
                callback: function(result, e){
                    if (result.data == true){
                        //결재상신 되었습니다.
                    }
                },
                errCallback: function(result, e) {
                    com.showMessage("CFE0112"); //작업 중 에러가 발생하였습니다. 관리자에게 문의하십시오
                }
            });
        }
    }


 	/**************************************************************************
	 * 함 수 명: scwin.fnSiteValidate
	 * 기 능: 현장 검색조건 관련 Validate
	 **************************************************************************/
	scwin.fnSiteValidate = function() {
	    if ( dsTcmSiteMap.get("siteCd").length == 0){
	        com.showMessage("CFW0004", "현장");// $1을(를) 선택하십시오.
	        return false;
	    }
		// 검색영역의 현장코드와 비교
		if (pMap.get("siteCd") != dsTcmSiteMap.get("siteCd")) {
	        com.showMessage("EMW0004", "검색항목에 변경된 현장");// $1을(를) 조회 하십시오.
	        return false;
		}
	    return true;
	};

  	/**************************************************************************
	 * 함 수 명: scwin.btnCntrtView_onclick
	 * 기 능: 이력 버튼 클릭
	 **************************************************************************/
	scwin.btnCntrtView_onclick = function() {
		// 현장조건 Check
		if (!scwin.fnSiteValidate()) return false;

        var popOps = {
            popup_name: "계약변경이력",
            url: "/ux/cm/cm0500/CM05000010P.xml",
            data: {
                siteCd: dsTcmSiteMap.get("siteCd"),
                siteNm: dsTcmSiteMap.get("siteNm")
            },
            widType: "L",
            height: "500",
            callbackFn: ""
        };
        com.popup_open(popOps);
	};

	scwin.btnFolding1_onclick = function() {
        setTimeout(function(){
                com.autoHeight();   //그리드 채우기
            },
            0
        );
	};
	]]></script>

