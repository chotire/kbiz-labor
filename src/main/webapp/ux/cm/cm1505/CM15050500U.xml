/**************************************************************************
 * [1] onpageload(초기화포함)
 **************************************************************************/
    scwin.onpageload = function() {
        //세션의 현장코드를 셋팅
        cmCom.setSite(wfmKeySiteInfo.getWindow());
        com.selCdListVrbl("sbxItexpGb", {"upperCd": "CM004100","setCd": "charValN1"});
        com.commonUI();
        
        grdTcmActbgtList.setColumnVisible("plActbgtQty", false);
        grdTcmActbgtList.setColumnVisible("plActbgtUct", false);
        grdTcmActbgtList.setColumnVisible("plActbgtAmt", false);
        grdTcmActbgtList.setColumnVisible("plActbgtRt", false);
        grdTcmActbgtList.setColumnVisible("plRemainAmt", false);
        grdTcmActbgtList.setColumnVisible("plPlAmt", false);
        
    };

/************************************************************************************
 *  [2] Event 함수
 ************************************************************************************/


    //현장코드 변경 이벤트
    scwin.WFrameSiteInfo_change_callback = function(){
        dsSearchMap.set("siteCd", wfmKeySiteInfo.getWindow().ibxKeySiteCd.getValue());  //현장 코드
        scwin.btnFind_onclick();
    };

	//조회 클릭
	scwin.btnFind_onclick = function() {
        dsSearchMap.set("siteCd", wfmKeySiteInfo.getWindow().ibxKeySiteCd.getValue());  //현장 코드

        if (dsSearchMap.get("siteCd") ==""){
            com.showMessage("SAA0031", "현장");     //을(를) 선택하십시오.
            return false;
        }

        ajaxLib.ajax("/cm/cm1505/CM15050025U/selectTcmSite", {
            method: "POST",
            requestData: dsSearchMap.getJSON(),
            callback: function(result, e){
                dsTcmActbgtList.setJSON(result.data.selectTcmActbgtAdJustList);
                com.setUdcValue("dsTotalCount", "udcListCount", dsTcmActbgtList.getTotalRow());
                btnTreeSpanAll.setValue("전체 접기");
            },
            errCallback: function(result, e) {
                com.showMessage("CFE0112"); //작업 중 에러가 발생하였습니다. 관리자에게 문의하십시오
            }
        });
	};

    //엑셀다운 클릭
	scwin.btnExcelDwn_onclick = function() {
        grdTcmActbgtList.advancedExcelDownload({
            fileName: "실행예산조회.xlsx",
            //useStyle: "true",
            indent: 1,
            useSubTotal : true,             // Sub-Total 표시 
            headerFontColor : "#FFFFFF",    // Header Font Color
            headerColor : "#D9D9D9",        // Header Back Color
            subTotalColor : "#FCE4D6",      // SubTotal Back Color
            footerColor : "#BDD7EE"         // Footer Color
        });
	};

    //엑셀다운(전체) 클릭
	scwin.btnExcelDwnFull_onclick = function() {
        grdTcmActbgtList.spanAll(true);
		grdTcmActbgtList.advancedExcelDownload({
            fileName: "실행예산조회_전체.xlsx",
            //useStyle: "true",
            indent: 1,
            useSubTotal : true,             // Sub-Total 표시 
            headerFontColor : "#FFFFFF",    // Header Font Color
            headerColor : "#D9D9D9",        // Header Back Color
            subTotalColor : "#FCE4D6",      // SubTotal Back Color
            footerColor : "#BDD7EE"         // Footer Color
        });
	};

	scwin.dsTcmActbgtList_ondataload = function() {
        //공종인 경우 글자색을 파란색으로.
        for( var i=0; i < dsTcmActbgtList.getTotalRow(); i++){
            if(dsTcmActbgtList.getCellData(i, "itemiYn") == "N"){
                grdTcmActbgtList.setRowColor(i , "blue");
            }
        }
	};

    //전체 펼치기, 접기 클릭
	scwin.btnTreeSpanAll_onclick = function() {
        if (dsTcmActbgtList.getTotalRow() <= 0) {
            return false;
        }

        if (btnTreeSpanAll.getValue() == "전체 펼치기"){
            btnTreeSpanAll.setValue("전체 접기");
            grdTcmActbgtList.spanAll(true);
        } else {
            btnTreeSpanAll.setValue("전체 펼치기");
            grdTcmActbgtList.spanAll(2);
        }
	};

    //내역조회팝업 클릭
	scwin.btnItemPopup_onclick = function() {
        var popOps = {
            popup_name: "내역조회",
            url: "/ux/cm/cm1505/CM15050010P.xml",
            data: {
                "siteCd": dsSearchMap.get("siteCd"),
                "gubun": "2"
            },
            widType: "L",
            height: "678",
            callbackFn: "scwin.fnItemPopupReturn"
        };
        com.popup_open(popOps);
	};

    //내역조회팝업 리턴 이벤트
    scwin.fnItemPopupReturn = function(obj){
        var nIndex = dsTcmActbgtList.getMatchedIndex("bgtItemiCd", obj.bgtItemiCd, true, 0, dsTcmActbgtList.getTotalRow());
        grdTcmActbgtList.setFocusedCell(nIndex[0] , 0, false);
    };
	]]></script>

