/**************************************************************************
 * [1] onpageload(초기화포함)
 **************************************************************************/
    scwin.onpageload = function() {
        if (typeof reqJsonParam == 'undefined') {
            if (com.isEmpty(gcm.tab_main)) {
                dsSearchMap.setJSON(JSON.parse($w.getPopupParam()).data);
            } else {
                dsSearchMap.setJSON(com.getAllParam());
            }
        } else {
            var jsonArray = JSON.parse(reqJsonParam);
            $.each(jsonArray, function(idx, item){
                dsSearchMap.setJSON(item);
            });
        }

        com.selCdListVrbl("grdTcmDirmgExctItemiList:itexpGb", {
                "upperCd": "CM004100",
                "setCd": "charValN1"
            },
            scwin.fnSearch()
        );
        com.commonUI();
        $("body").css("overflow","scroll");
    };

/************************************************************************************
 *  [2] 사용자 정의 함수
 ************************************************************************************/

    //조회
    scwin.fnSearch = function() {
        ajaxLib.ajax("/cm/cm1505/CM15051010/notifyDirmgExctNoList", {
            method: "POST",
            requestData: dsSearchMap.getJSON(),
            callback: function(result, e){

                dsTcmDirmgExctItemiList.setJSON(result.data.selectDirmgExctNoList);
                dsSearchMap.set("aprvTtl", result.data.selectTcmDirmgExctApro.aprvTtl);
                dsSearchMap.set("dirmgExctAproCd", result.data.selectTcmDirmgExctApro.dirmgExctAproCd);
                com.setUdcValue("dsTotalCount", "udcListCount", dsTcmDirmgExctItemiList.getTotalRow());

                //공종인 경우 파란색 폰트와 금액은 모두 null 처리.
                for (var i=0; i<dsTcmDirmgExctItemiList.getTotalRow(); i++){
                    if (dsTcmDirmgExctItemiList.getCellData(i, "itemiYn") == "N"){
                        grdTcmDirmgExctItemiList.setRowColor(i, "blue");
                        dsTcmDirmgExctItemiList.setRowJSON(i, {
                                contQty: null,
                                contUct: null,
                                contAmt: null,
                                actbgtQty: null,
                                actbgtUct: null,
                                actbgtAmt: null,
                                aproQty: null,
                                aproUct: null,
                                aproAmt: null,
                                reduceUct: null,
                                reduceAmt: null,
                                contAproRt: null,
                                contActbgtRt: null
                            },
                            true
                        );
                    }
                }
            },
            errCallback: function(result, e){
                com.showMessage("CFE0112"); //작업 중 에러가 발생하였습니다. 관리자에게 문의하십시오
            }
        });
    };


/************************************************************************************
 *  [3] Event 함수
 ************************************************************************************/

	]]></script>


