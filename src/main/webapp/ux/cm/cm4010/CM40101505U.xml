 * ========================================
 *
 **************************************************************************/
 
    /************************************************************************************
     *  [1] onpageload
     ************************************************************************************/
    scwin.onpageload = function() {
        scwin.fnInitial();
    };

    /** 화면초기화 **/
    scwin.fnInitial = function() {

        com.selCdList( "grdTcmLbctPayItemiList:erndGbCd"	, "CM005001", "Y", "Y");  			//근로구분코드
        com.selCdList( "grdTcmLbctPayItemiList:constBankCd"	, "CF016"	, "Y");  				//은행코드

        //초기값 설정
        com.setUdcValue("udcFrYymm"	, "calYearMonth", $p.getCurrentServerDate( "yyyyMM" ));  	//출연년월
        dsSrchMap.set("keyFrYymm"	, com.getUdcValue("udcFrYymm", "calYearMonth"));   			//출연년월
        com.setUdcValue("udcToYymm"	, "calYearMonth", $p.getCurrentServerDate( "yyyyMM" ));  	//출연년월
        dsSrchMap.set("keyToYymm"	, com.getUdcValue("udcToYymm", "calYearMonth"));   			//출연년월

        com.selCdList( "keyErndGbCd", "CM005001", "Y", "Y");      //근로구분코드

        scwin.fnBtnDisabled();
        //UI팀용 함수 호출
        scwin.commonUI();
    };

    /************************************************************************************
     *  [2] 전역변수
     ************************************************************************************/
    var svcId = "";

    /************************************************************************************
     *  [3] Event 함수
     ************************************************************************************/

    // 검색 버튼
    scwin.btnFind_onclick = function() {
        scwin.fnSearchList();
    };

    //엑셀다운 버튼
    scwin.btnExcelDwn_onclick = function() {
        if(dsTcmLbctPayItemiList.getTotalRow() <= 0){
            com.showMessage("CMW0042");     //다운로드할 내역이 존재하지 않습니다.
            return;
        }else{
            var excelOpt = {};  						//excel download 옵션
            excelOpt.fileName = "노무비지급현황.xlsx";    	//또는 xlsx 확장자 사용
            grdTcmLbctPayItemiList.advancedExcelDownload( excelOpt );
        }
    };

    /**************************************************************************
     *  [4] Callback
     **************************************************************************/

    // 결과 후처리
    scwin.fnCallback = function(result, e) {
        switch (svcId) {
            case "selectTcmLbctPayItemiList" :
                dsTcmLbctPayItemiList.setJSON(result.data.tcmLbctPayItemiList);               //그리드 검색 결과
                break;
        }
    };

    // 에러 후처리
    scwin.fnErrCallBack = function(e) {
        switch (svcId) {
            case "selectTcmLbctPayItemiList" :
                console.log('조회오류: ' + e);
                break;
        }
    }

    /**************************************************************************
     *  [5] 사용자정의 함수
     **************************************************************************/

    // 조회 List
    scwin.fnSearchList = function() {

        //DS 매핑
        dsSrchMap.set("keyFrYymm", com.getUdcValue("udcFrYymm", "calYearMonth"));      //출연기간 from
        dsSrchMap.set("keyToYymm", com.getUdcValue("udcToYymm", "calYearMonth"));      //출연기간 to

        if( dsSrchMap.get("keyFrYymm").length == 0  ){
            var focusId = com.getUdcComp("udcFrYymm", "calYearMonth").id;
            com.showMessage("CMW0025", "출연기간", "", "", focusId); //$1은(는) 필수입력사항입니다.

            return ;
        }

        if( dsSrchMap.get("keyToYymm").length == 0  ){
            var focusId = com.getUdcComp("udcToYymm", "calYearMonth").id;
            com.showMessage("CMW0025", "출연기간", "", "", focusId); //$1은(는) 필수입력사항입니다.

            return ;
        }

        svcId = "selectTcmLbctPayItemiList";

        dsSrchMap.set("keyAprvPgmId"    , "CMDA0200AG" );

        ajaxLib.ajax("/cm/cm4010/CM40101505U/selectTcmLbctPayItemiList", {
            mode : "asynchronous",
            method : "POST",
            requestData : dsSrchMap.getJSON(),
            callback : "fnCallback",
            errCallback : "fnErrCallBack"
        });

    };
]]></script>

