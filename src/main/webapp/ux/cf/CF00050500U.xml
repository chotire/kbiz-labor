						<w2:column dataType="text" id="deptNm" name="부서명" />
						<w2:column dataType="text" id="jbclCd" name="직급코드" />
						<w2:column dataType="text" id="jbclNm" name="직급명" />
						<w2:column dataType="text" id="jbposGbCd" name="직위구분코드" />
						<w2:column dataType="text" id="jbposGbNm" name="직위구분명" />
						<w2:column dataType="text" id="dsgnCd" name="호칭코드" />
						<w2:column dataType="text" id="dsgnNm" name="호칭명" />
						<w2:column dataType="text" id="empGbCd" name="사원구분코드" />
						<w2:column dataType="text" id="incoTelNo" name="사내전화번호" />
						<w2:column dataType="text" id="secCrryTelNo" name="SEC_휴대전환번호" />
						<w2:column dataType="text" id="email" name="이메일" />
						<w2:column dataType="text" id="useYn" name="사용여부" />
						<w2:column dataType="text" id="useValdStrtDt" name="사용유효시작일자" />
						<w2:column dataType="text" id="useValdEndDt" name="사용유효종료일자" />
						<w2:column dataType="text" id="bdyDt" name="생년월일_일자" />
						<w2:column dataType="text" id="solaLunaGb" name="양력음력구분" />
						<w2:column dataType="text" id="wdanDt" name="결혼기념일_일자" />
						<w2:column dataType="text" id="photoPathNm" name="사진_경로_명" />
						<w2:column dataType="text" id="userTypeGb" name="사용자_유형_구분" />
						<w2:column dataType="text" id="rmkCn" name="비고_내용" />
						<w2:column dataType="text" id="pwErrTms" name="비밀번호 오류 횟수" />
						<w2:column dataType="text" id="fstRegprId" name="등록자_사용자_ID" />
						<w2:column dataType="date" id="fstRegDtm" name="등록_일시" />
						<w2:column dataType="text" id="fnlModprId" name="수정자_사용자_ID" />
						<w2:column dataType="date" id="fnlModiDtm" name="수정_일시" />
						<w2:column dataType="text" id="empGbNm" name="사원구분명" />
						<w2:column dataType="text" id="fstRegprEmpnoNm" name="최초_등록자_명" />
						<w2:column dataType="text" id="fnlModprEmpnoNm" name="최종_수정자_명" />
					</w2:columnInfo>
				</w2:dataList>
				<w2:dataMap baseNode="map" id="dsUserInfo">
				    <w2:keyInfo>
				        <w2:key dataType="text" id="userId" name="사용자ID" />
				        <w2:key dataType="text" id="userNm" name="사용자명" />
				        <w2:key dataType="text" id="userEmpno" name="사용자사번" />
				        <w2:key dataType="text" id="deptCd" name="부서코드" />
				        <w2:key dataType="text" id="deptNm" name="부서명" />
				        <w2:key dataType="text" id="jbposGbNm" name="직위구분명" />
				        <w2:key dataType="text" id="empGbCd" name="사원구분코드" />
				        <w2:key dataType="text" id="secCrryTelNo" name="SEC_휴대전환번호" />
				        <w2:key dataType="text" id="email" name="이메일" />
				        <w2:key dataType="text" id="useYn" name="사용여부" />
				        <w2:key dataType="text" id="useValdStrtDt" name="사용유효시작일자" />
				        <w2:key dataType="text" id="useValdEndDt" name="사용유효종료일자" />
				        <w2:key dataType="text" id="rmkCn" name="비고_내용" />
				        <w2:key dataType="text" id="fstRegprId" name="등록자_사용자_ID" />
				        <w2:key dataType="text" id="fnlModprId" name="수정자_사용자_ID" />
				        <w2:key id="userTypeGb" name="사용자유형구분" dataType="text"></w2:key>
				        <w2:key id="pwEnc" name="비밀번호_암호화" dataType="text"></w2:key>
				        <w2:key id="jbclCd" name="직급코드" dataType="text"></w2:key>
				        <w2:key id="jbclNm" name="직급명" dataType="text"></w2:key>
				    </w2:keyInfo>
				</w2:dataMap>
				<w2:dataList baseNode="list" id="dsUseYn" repeatNode="map" saveRemovedData="true">
					<w2:data use="true">
						<w2:row>
							<cd>
								<![CDATA[Y]]>
							</cd>
							<cdNm>
								<![CDATA[Y]]>
							</cdNm>
						</w2:row>
						<w2:row>
							<cd>
								<![CDATA[N]]>
							</cd>
							<cdNm>
								<![CDATA[N]]>
							</cdNm>
						</w2:row>
					</w2:data>
					<w2:columnInfo>
						<w2:column dataType="text" id="cd" name="코드" />
						<w2:column dataType="text" id="cdNm" name="코드명" />
					</w2:columnInfo>
				</w2:dataList>
				<w2:dataList baseNode="list" id="dsEmpGbCd" repeatNode="map" saveRemovedData="true">
					<w2:columnInfo>
						<w2:column dataType="text" id="cd" name="코드" />
						<w2:column dataType="text" id="cdNm" name="코드명" />
					</w2:columnInfo>
					<w2:data use="true">
						<w2:row>
							<cd>
								<![CDATA[10]]>
							</cd>
							<cdNm>
								<![CDATA[현장직]]>
							</cdNm>
						</w2:row>
						<w2:row>
							<cd>
								<![CDATA[20]]>
							</cd>
							<cdNm>
								<![CDATA[사무직]]>
							</cdNm>
						</w2:row>
					</w2:data>
				</w2:dataList>
			</w2:dataCollection>
			<w2:workflowCollection />
		</xf:model>

		<script type="javascript" lazy="false"><![CDATA[
    /***************************************************************************
     * Program ID : CF00050500U.xml 
     * Program Name : 미계정 사용자 관리 
     * Author : 김환섭 (tpkhs1)
     * Date : 2019.02.15
     * 
     * History 
     * ======================================== 
     * 변경일자     	  수정자   내 용 
     * ======================================== 
     * 2019.02.15   김환섭   최초작성
     *     
     **************************************************************************/
    /************************************************************************************
     *  [1] 전역변수 
     ************************************************************************************/
    var svcId = "";
	var initPw = "sdcit911!";
    var pageRowStart = 0; 	//페이지 시작행번호
    var pageRowCount = 50; 	//페이지 요청건수

    /************************************************************************************ 
     *  [2] onpageload(초기화포함)
     ************************************************************************************/
    scwin.onpageload = function () {

        scwin.initScreen();

        com.getUdcComp("udcDeptInfo2", "deptNm").removeClass("req");		//소속부서UDC_Input RemoveClass처리
        com.selCdList("sbxEmpGbCd", "CF013", "Y", "N", "", "", "", "", "", "", "", "", "", "");

        // 소노관리자인 경우에만 활성화
        if (sessionStorage.getItem("sonoAdminYn") == "Y") {
            binPwInit.setDisabled( false );
        } else {
            binPwInit.setDisabled( true );
        }    


    };

    // 화면 초기화(필수)
    scwin.initScreen = function () {
        grdUserList.initGrid(); 					//사용자목록 초기화
        com.setUdcValue("uListCount", "udcListCount", 0);
    	pageRowStart = 0;							//페이지 시작행번호

        grpUserInfo.init();							//사용자정보 초기화
   		ibxKeyCrryTelNo.addClass( "hide" );			//휴대폰 hide
   		sbxKeyUserTypeGb.addClass( "hide" );		//사용자유형 hide(조회값)
   		grpUserTypeGb.addClass( "hide" )			//사용자유형 hide(INPUT값)
        com.setUdcValue("udcPrdInfo", "frDate", com.transDateType("now"));
        com.setUdcValue("udcPrdInfo", "toDate", "99991231");
        rdoUseYn.setSelectedIndex( 0 );				//사용여부Set

		//if (gcm.MENU_ID == "MNU_CF_102010") {
            labTitle.setLabel("사용자유형");
			sbxKeyUserTypeGb.removeClass( "hide" );	//사용자유형 show(조회값)
	   		grpUserTypeGb.removeClass( "hide" )		//사용자유형 show(INPUT값)
		//} else {
        //    labTitle.setLabel("휴대폰");
		//	ibxKeyCrryTelNo.removeClass( "hide" );	//휴대폰 show
		//	sbxKeyUserTypeGb.setValue( "5" );		//사용자유형(조회값)을 강제 5:AS사용자로 셋팅
		//	sbxUserTypeGb.setValue( "5" );			//사용자유형(INPUT값)을 강제 5:AS사용자로 셋팅
		//}
    };

    /**************************************************************************
     *  [3] 사용자정의 함수
     **************************************************************************/
    // 조회 List
    scwin.fnSearchList = function () {
        // udc의 코드값 가져오기.
        dsSrch.set("deptCd", com.getUdcValue("udcDeptInfo2", "deptCd"));

        svcId = "selectTcfUserInfoList";

        ajaxLib.ajax("/cf/CF00050500U/selectTcfUserInfoList", {
            mode: "asynchronous",
            method: "POST",
            requestData: dsSrch.getJSON(),
            callback: "fnCallback",
            errCallback: "fnErrCallBack"
        });
    };

    // 삭제
    scwin.fnDelete = function () {
        svcId = "deleteTcfUserInfo";
        if (confirm("삭제 하시겠습니까?")) {

            ajaxLib.ajax("/cf/CF00050500U/deleteTcfUserInfo", {
                mode: "asynchronous",
                method: "Delete",
                requestData: dsUserInfo.getJSON(),
                callback: "fnCallback",
                errCallback: "fnErrCallBack"
            });
        }
    };

    // 저장
    scwin.fnUpdate = function () {
        svcId = "updateTcfUserInfo";
        if (confirm("저장 하시겠습니까?")) {

            ajaxLib.ajax("/cf/CF00050500U/updateTcfUserInfo", {
                mode: "asynchronous",
                method: "POST",
                requestData: dsUserInfo.getJSON(),
                callback: "fnCallback",
                errCallback: "fnErrCallBack"
            });
        }
    };

    // 저장시 Validate Check
    scwin.fnValidate = function () {
		//data유효성체크(전체)
		if (!com.validateGroup(grpUserInfo)) {
			return false;
		};

		//사용여부 체크
        if (strLib.isEmpty(rdoUseYn.getValue())) {
            com.showMessage("CFW0003", "사용여부", "", "", rdoUseYn.id);	// $1 항목을 확인하십시오.
            return false;
        }
        
        return true;
    };

    /**************************************************************************
     *  [4] Callback
     **************************************************************************/
    // 결과 후처리
    scwin.fnCallback = function (result, e) {

        switch (svcId) {
            case "selectTcfUserInfoList":
                dsUserList.setJSON(result.data.dsTcfUserInfoList, true);
                com.setUdcValue("uListCount", "udcListCount", dsUserList.getTotalRow());
                pageRowStart = pageRowStart + pageRowCount;		//다음 조회갯수 Setting
                break;

            case "updateTcfUserInfo":
                dsUserInfo.setEmptyValue();
				com.showMessage("CSA0004", "사용자", "저장");	//정상적으로 $1이(가) $2되었습니다.

		    	//초기화
		        scwin.initScreen();
				dsSrch.set("pageRowStart", pageRowStart);
				dsSrch.set("pageRowCount", pageRowCount);
                scwin.fnSearchList();
                break;

            case "deleteTcfUserInfo":
                dsUserInfo.setEmptyValue();
				com.showMessage("CSA0004", "사용자", "삭제");	//정상적으로 $1이(가) $2되었습니다.

		    	//초기화
		        scwin.initScreen();
				dsSrch.set("pageRowStart", pageRowStart);
				dsSrch.set("pageRowCount", pageRowCount);
                scwin.fnSearchList();
                break;

            case "pwInit":
				com.showMessage("SAA0032");						//정상적으로 처리되었습니다.
                break;
        }
    };

    // 에러 후처리
    scwin.fnErrCallBack = function (e) {
        switch (svcId) {
            case "selectTcfUserInfoList":
                break;

            case "updateTcfUserInfo":
                break;

            case "deleteTcfUserInfo":
                break;
        }
    };

    /************************************************************************************
     *  [5] Event 함수 
     ************************************************************************************/
    /**************************************************************************
     * Event: scwin.btnFind_onclick
     * 기 능: 조회버튼 클릭시 처리
     **************************************************************************/
    scwin.btnFind_onclick = function () {
    	//초기화
        scwin.initScreen();

		dsSrch.set("pageRowStart", pageRowStart);
		dsSrch.set("pageRowCount", pageRowCount);

		//조회
        scwin.fnSearchList();
    };

    /**************************************************************************
     * Event: scwin.grdUserList_onscrollend
     * 기 능: 그리드 scrollend시 처리
     **************************************************************************/
	scwin.grdUserList_onscrollend = function() {
		//총건수가 (페이지 시작행번호 + 페이지 요청건수) 보다 작으면 SKIP
		if (dsUserList.getTotalRow() < (dsSrch.get("pageRowStart") + dsSrch.get("pageRowCount"))){
			return;
		}

		dsSrch.set("pageRowStart", pageRowStart);
		dsSrch.set("pageRowCount", pageRowCount);
		
		//조회
        scwin.fnSearchList();
	};

    /**************************************************************************
     * Event: scwin.btnNew_onclick
     * 기 능: 신규버튼 클릭시 처리
     **************************************************************************/
	scwin.btnNew_onclick = function() {
        grpUserInfo.init();							//사용자정보 초기화
   		grpUserTypeGb.addClass( "hide" )			//사용자유형 hide(INPUT값)
        com.setUdcValue("udcPrdInfo", "frDate", com.transDateType("now"));
        com.setUdcValue("udcPrdInfo", "toDate", "99991231");
        rdoUseYn.setSelectedIndex( 0 );				//사용여부Set

		if (gcm.MENU_ID == "MNU_CF_102010") {
	   		grpUserTypeGb.removeClass( "hide" )		//사용자유형 show(INPUT값)
		} else {
			sbxKeyUserTypeGb.setValue( "5" );		//사용자유형(조회값)을 강제 5:AS사용자로 셋팅
			sbxUserTypeGb.setValue( "5" );			//사용자유형(INPUT값)을 강제 5:AS사용자로 셋팅
		}
	};

    /**************************************************************************
     * Event: scwin.btnSave_onclick
     * 기 능: 저장버튼 클릭시 처리
     **************************************************************************/
    scwin.btnSave_onclick = function () {
        //udc의 코드값 가져오기.
        dsUserInfo.set("deptCd", com.getUdcValue("udcDeptInfo", "deptCd"));
        dsUserInfo.set("deptNm", com.getUdcValue("udcDeptInfo", "deptNm"));
        dsUserInfo.set("useValdStrtDt", com.getUdcValue("udcPrdInfo", "frDate"));
        dsUserInfo.set("useValdEndDt", com.getUdcValue("udcPrdInfo", "toDate"));
        //신규시 초기비밀번호 셋팅
        dsUserInfo.set("pwEnc", initPw);

        if (scwin.fnValidate()) {
            scwin.fnUpdate();
        }
    };

    /**************************************************************************
     * Event: scwin.btnDel_onclick
     * 기 능: 삭제버튼 클릭시 처리
     **************************************************************************/
    scwin.btnDel_onclick = function () {
        scwin.fnDelete();
    };

    /**************************************************************************
     * Event: scwin.grid_onrowindexchange
     * 기 능: 그리드 row 변경시
     **************************************************************************/
    scwin.grid_onrowindexchange = function (row, oldRow) {

        var data = dsUserList.getRowJSON(row);
        // Binding
        dsUserInfo.setJSON(data)

        com.setUdcValue("udcDeptInfo", "deptCd", data.deptCd);
        com.setUdcValue("udcDeptInfo", "deptNm", data.deptNm);

        com.setUdcValue("udcPrdInfo", "frDate", data.useValdStrtDt);
        com.setUdcValue("udcPrdInfo", "toDate", data.useValdEndDt);
    };
	
    /**************************************************************************
     * Event: scwin.binPwInit_onclick
     * 기 능: 비밀번호 초기화 클릭시 처리
     **************************************************************************/
    scwin.binPwInit_onclick = function () {
        var param = "";
        var userEmpno = dsUserInfo.get("userEmpno");

        // 사용자를 선택유무 체크
        if (userEmpno.length == 0) {
			com.showMessage("CFW0004", "사용자");	//$1을(를) 선택하십시오.
            return;
        }

        if (confirm("비밀번호 초기화 [" + initPw + "] 하시겠습니까?")) {
            svcId = "pwInit";
            param = {
                "userEmpno": userEmpno,
                "keyNewPwEnc": initPw,
                "keyNewPwEncConfirm": initPw,
                "keyPwType": "1"
            }
            ajaxLib.ajax("/cf/CF00050001U/putTcfTmpUserPwMng", {
                mode: "asynchronous",
                method: "POST",
                requestData: JSON.stringify(param),
                callback: "fnCallback",
                errCallback: "fnErrCallBack"
            });
        }
    };

    /**************************************************************************
     * Event: scwin.ibxKeyUserNm_onkeyup
     * 기 능: 성명 입력후 Enter시 처리
     **************************************************************************/
    scwin.ibxKeyUserNm_onkeyup = function (e) {
        if (e.keyCode == '13') {
            scwin.btnFind_onclick();
        }
    };

    /**************************************************************************
     * Event: scwin.ibxKeyCrryTelNo_onkeyup
     * 기 능: 휴대폰 입력후 Enter시 처리
     **************************************************************************/
    scwin.ibxKeyCrryTelNo_onkeyup = function (e) {
        if (e.keyCode == '13') {
            scwin.btnFind_onclick();
        }
    };
]]></script>
