    <script type="javascript" lazy="false"><![CDATA[
    /***************************************************************************
     * Program ID : CF00353500U.xml 
     * Program Name : 공휴일 관리
     * Author : 김환섭 (tpkhs1)
     * Date : 2019.03.15
     * 
     * History 
     * ======================================== 
     * 변경일자     수정자   내 용 
     * ======================================== 
     * 2019.03.15   김환섭	최초작성
     *     
     **************************************************************************/

    /************************************************************************************
     *  [1] 전역변수 
     ************************************************************************************/
    var svcId = "";
    var pageRowStart = 0; 	//페이지 시작행번호
    var pageRowCount = 50; 	//페이지 요청건수

    /************************************************************************************ 
     *  [2] onpageload
     ************************************************************************************/
    scwin.onpageload = function () {
        grpSearch.init();		//조회그룹 초기화
        scwin.initScreen();		//초기화

        com.selCdList("sbxSolaLunaGb", "CF044");				//양력음력구분[CF044]
		dsCdCF046.setJSON(com.sessionTcfCodeSearch("CF046"));	//요일구분코드[CF046]
    };

	// 초기화
    scwin.initScreen = function () {
        grdClnrList.initGrid(); //달력목록 초기화
        dsClnrList.reform();	//달력수정목록 초기화

        com.setUdcValue("uListCount", "udcListCount", 0);
    	pageRowStart = 0;		//페이지 시작행번호
    };

    /**************************************************************************
     *  [3] 사용자정의 함수
     **************************************************************************/
    // 조회 List
    scwin.fnSearchList = function () {
		dsSrch.set("pageRowStart", pageRowStart);
		dsSrch.set("pageRowCount", pageRowCount);

        svcId = "selectTcfClnrList";

        ajaxLib.ajax("/cf/CF00353500U/selectTcfClnrList", {
            mode: "asynchronous",
            method: "POST",
            requestData: dsSrch.getJSON(),
            callback: "fnCallback",
            errCallback: "fnErrCallBack"
        });

    };

    // 저장 처리
    scwin.fnUpdate = function () {
        if (confirm("저장 하시겠습니까?")) {
            svcId = "updateTcfClnr";

            ajaxLib.ajax("/cf/CF00353500U/updateTcfClnr", {
                mode: "asynchronous",
                method: "POST",
                requestData: {
                    param: dsSrch.getJSON(),
                    tcfClnrList: dsClnrList.getModifiedJSON()
                },
                callback: "fnCallback",
                errCallback: "fnErrCallBack"
            });
        }
    };

    /**************************************************************************
     *  [4] Callback
     **************************************************************************/
    // 결과 후처리
    scwin.fnCallback = function (result, e) {
        switch (svcId) {
            case "selectTcfClnrList":
                dsClnrList.setJSON(result.data.dsTcfClnrList, true);
                com.setUdcValue("uListCount", "udcListCount", dsClnrList.getTotalRow());
                pageRowStart = pageRowStart + pageRowCount;	//다음 조회갯수 Setting
		        dsClnrList.reform();						//달력수정목록 초기화

                break;

            case "updateTcfClnr":
				com.showMessage("SCN0002", "저장");			//$1 되었습니다.

                scwin.btnFind_onclick();
                break;
        }
    };

    // 에러 후처리
    scwin.fnErrCallBack = function (e) {
    }

    /************************************************************************************
     *  [5] Event 함수 
     ************************************************************************************/
    // 조회시 처리
    scwin.btnFind_onclick = function () {
    	scwin.initScreen();		//초기화

        scwin.fnSearchList();
    };

    // 그리드 scrollend시 처리
	scwin.grdClnrList_onscrollend = function() {
		//총건수가 (페이지 시작행번호 + 페이지 요청건수) 보다 작으면 SKIP
		if (dsClnrList.getTotalRow() < (dsSrch.get("pageRowStart") + dsSrch.get("pageRowCount"))){
			return;
		}

        scwin.fnSearchList();
	};

    // 저장시 처리
	scwin.btnSave_onclick = function() {
	    var cnt = dsClnrList.getModifiedJSON().length;
	    if (cnt <= 0) {
	        com.showMessage("CMW0024");	//	저장(변경)할 내용이 없습니다.
	        return;
	    }
		
        scwin.fnUpdate();
	};
	]]></script>
