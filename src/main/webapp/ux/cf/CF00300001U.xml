		<script type="javascript" lazy="false"><![CDATA[
    /***************************************************************************
     * Program ID : CF00300001U.xml 
     * Program Name : 거래처 권한 관리 
     * Author : 김환섭 (tpkhs1)
     * Date : 2019.02.15
     * 
     * History 
     * ======================================== 
     * 변경일자     	  수정자   내 용 
     * ======================================== 
     * 2019.02.15   김환섭   최초작성
     *     
     **************************************************************************/

    /************************************************************************************
     *  [1] 전역변수 
     ************************************************************************************/
    var svcId = "";
    var ingStatus = "";
    var pageRowStart = 0; 	//페이지 시작행번호
    var pageRowCount = 50; 	//페이지 요청건수

    /************************************************************************************ 
     *  [2] onpageload(초기화포함)
     ************************************************************************************/
    scwin.onpageload = function () {
        scwin.initScreen();
        com.selCdList("sbxKeyEnprGbCd", "CF009", "Y");		//기업구분[CF009]
		//조회용으로만 사용하므로 막음
		btnSel.addClass('hide');
		btnAddRow.addClass('hide');
		btnDelRow.addClass('hide');
    };

    // 화면 초기화(필수)
    scwin.initScreen = function () {
        grdTradeList.initGrid(); 	//거래처목록 초기화
        grdUseGrpList.initGrid(); 	//거래처권한목록 초기화
        com.setUdcValue("udcListCount", "udcListCount", 0);
    	pageRowStart = 0;			//페이지 시작행번호
    };

    /**************************************************************************
     *  [3] 사용자정의 함수
     **************************************************************************/
    // 조회 List
    scwin.fnSearchList = function () {
        svcId = "selectTcfTradeInfoPageList";

        ajaxLib.ajax("/cf/CF00300000U/selectTcfTradeInfoPageList", {
            mode: "asynchronous",
            method: "POST",
            requestData: dsSrch.getJSON(),
            callback: "fnCallback",
            errCallback: "fnErrCallBack"
        });

    };

    scwin.fnSearchUseGrp = function () {
        svcId = "selectTcfTradeUseGrpList";

        ajaxLib.ajax("/cf/CF00300001U/selectTcfTradeUseGrpList", {
            mode: "asynchronous",
            method: "POST",
            requestData: dsSrchUseGrp.getJSON(),
            callback: "fnCallback",
            errCallback: "fnErrCallBack"
        });

    };


    scwin.fnUpdate = function () {

        if (confirm("수정 하시겠습니까?")) {

            ajaxLib.ajax("/cf/CF00300001U/updateTcfTradeUseGrp", {
                mode: "asynchronous",
                method: "POST",
                requestData: {
                    tcfTradeUseGrpList: dsUseGrpList.getAllJSON()
                },
                callback: "fnCallback",
                errCallback: "fnErrCallBack"
            });
        }

    };
    scwin.fnComCdPop = function () {
        var popOps = {
            popup_name: "공통코드 검색",
            url: "/ux/cf/CF00000004P.xml",
            data: {
                upperCd: "CF048",
                cdNm: ""
            },
            widType: "M",
            height: "520",
            callbackFn: "scwin.popCallback"
        };
        com.popup_open(popOps);

    }
    // 팝업 결과 후처리
    scwin.popCallback = function (arg) {

        dsTcfCodeList.setJSON(arg);
        var totCnt = dsTcfCodeList.getTotalRow();

        var pRowIndex = 0;
        var pTradeCd = "";
        var cRowIndex = 0;

        pRowIndex = grdTradeList.getFocusedRowIndex();
        if (pRowIndex == -1) {
            return;
        } else {
            pTradeCd = dsTradeList.getCellData(pRowIndex, "tradeCd")

            if (totCnt > 0) {
                for (var i = 0; i < totCnt; i++) {
                    cRowIndex = dsUseGrpList.insertRow();
                    dsUseGrpList.setCellData(cRowIndex, "tradeCd", pTradeCd);
                    dsUseGrpList.setCellData(cRowIndex, "useGrpCd", dsTcfCodeList.getCellData(i, "cd"));
                    dsUseGrpList.setCellData(cRowIndex, "useGrpCdNm", dsTcfCodeList.getCellData(i, "cdNm"));
                }
            }
        }
    };

    /**************************************************************************
     *  [4] Callback
     **************************************************************************/
    // 결과 후처리
    scwin.fnCallback = function (result, e) {

        switch (svcId) {
            case "selectTcfCodeComboList":

                alert("정상 조회되었습니다.");
                alert("result.data.dsCodeComboList:" + result.data.dsCodeComboList);

                break;
            case "selectTcfCodeDtlPoolComboList":

                alert("Pool Code 정상 조회되었습니다.");
                alert("result.data.dsPoolComboList:" + result.data.dsPoolComboList);

                break;

            case "selectTcfTradeInfoPageList":
                dsTradeList.setJSON(result.data.dsTcfTradeInfoList, true);
                com.setUdcValue("udcListCount", "udcListCount", dsTradeList.getTotalRow());
                pageRowStart = pageRowStart + pageRowCount;		//다음 조회갯수 Setting

                ingStatus = "S";
                break;

            case "selectTcfTradeUseGrpList":

                dsUseGrpList.setJSON(result.data.dsTcfTradeUseGrpList);
                //totalCount.setLabel( dsTradeList.getTotalRow());
                ingStatus = "S";
                break;
        }
    };

    // 에러 후처리
    scwin.fnErrCallBack = function (e) {
        console.log(e);
    };

    /************************************************************************************
     *  [5] Event 함수 
     ************************************************************************************/
    scwin.btnFind_onclick = function () {
    	//초기화
        scwin.initScreen();

		dsSrch.set("pageRowStart", pageRowStart);
		dsSrch.set("pageRowCount", pageRowCount);
		
		//조회
        scwin.fnSearchList();
    };

    // scrollend시 처리
	scwin.grdTradeList_onscrollend = function() {
		//총건수가 (페이지 시작행번호 + 페이지 요청건수) 보다 작으면 SKIP
		if (dsTradeList.getTotalRow() < (dsSrch.get("pageRowStart") + dsSrch.get("pageRowCount"))){
			return;
		}

		dsSrch.set("pageRowStart", pageRowStart);
		dsSrch.set("pageRowCount", pageRowCount);
		
		//조회
        scwin.fnSearchList();
	};

    scwin.btnSaveOnClick = function () {
        alert("body scwin.btnCancelOnClick !!")
        scwin.fnUpdate();
    };
    scwin.btnCancelOnClick = function () {
        alert("body scwin.btnCancelOnClick !!")
    };

    scwin.btnDelRow_onclick = function () {
        alert("행삭제 버튼으로 테스트합니다.");
        var cRowIndex = grdUseGrpList.getFocusedRowIndex();
        dsUseGrpList.removeRow(cRowIndex);
    };

    scwin.btnSelOnClick = function () {
        var pRowIndex = 0;

        pRowIndex = grdTradeList.getFocusedRowIndex();
        if (pRowIndex == -1) {
            alert("거래처 목록을 선택 하십시오.");
            return;
        } else {
            scwin.fnComCdPop();
        }
    };

    scwin.OnRowChange = function (row, oldRow) {

        var tradeCd = dsTradeList.getCellData(row, "tradeCd");
        dsSrchUseGrp.set("tradeCd", tradeCd);

        scwin.fnSearchUseGrp();
    };

    scwin.btnAddRow_onclick = function () {
        scwin.fnAdd();
    };

    scwin.fnAdd = function () {
        var pRowIndex = 0;
        var pTradeCd = "";
        var cRowIndex = 0;

        pRowIndex = grdTradeList.getFocusedRowIndex();
        if (pRowIndex == -1) {} else {
            pTradeCd = dsTradeList.getCellData(pRowIndex, "tradeCd")

            cRowIndex = grdUseGrpList.getFocusedRowIndex();
            if (cRowIndex == -1) {
                cRowIndex = dsUseGrpList.insertRow();
                dsUseGrpList.setCellData(cRowIndex, "tradeCd", pTradeCd);
            } else {
                cRowIndex = cRowIndex + 1;
                dsUseGrpList.insertRow(cRowIndex);
                dsUseGrpList.setCellData(cRowIndex, "tradeCd", pTradeCd);
            }
        }
    };

    //거래처코드 엔터	
    scwin.keyTradeCd_onkeyup = function (e) {
        if (e.keyCode == 13) {
            btnFind.trigger("onClick");
        }
    };

    //거래처명 엔터
    scwin.keyTradeNm_onkeyup = function (e) {
        if (e.keyCode == 13) {
            btnFind.trigger("onClick");
        }
    };

    //사업자등록번호 엔터
    scwin.keyBzprRegNo_onkeyup = function (e) {
        if (e.keyCode == 13) {
            btnFind.trigger("onClick");
        }
    };

    //법인등록번호 엔터
    scwin.keyCorpRegNo_onkeyup = function (e) {
        if (e.keyCode == 13) {
            btnFind.trigger("onClick");
        }
    };

    scwin.btnDelRow_onclick = function () {
    };
]]></script>
